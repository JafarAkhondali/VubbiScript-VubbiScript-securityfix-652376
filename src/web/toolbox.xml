<toolbox>
    <category name="Basis" svg="true">
    
          <block type="unity_me"></block>
          
          <block type="unityEvents_start"></block>
          <block type="unityEvents_update"></block>
          <block type="unityInput_key"></block>
          
          <block type="unityRender_setLook">
            <value name="WHO"><block type="unity_me"></block></value>
          </block>
            
          <block type="unityEvents_mouse"></block>
        
        <category_moved name="Basis" svg="true">
        </category_moved>
        
        
        
        
        <category_moved name="Berichten">
          <block type="unityGeneral_msgSend"></block>
          <block type="unityGeneral_msgReceive"></block>
          <!--<block type="unityGeneral_msgSendWithArg"></block>
          <block type="unityGeneral_msgReceiveWithArg"></block>-->
        </category_moved>
    </category>
    
    
        
    <category name="Positie" svg="true">
      <block type="unityTransform_jumpto">
        <value name="WHO"><block type="unity_me"></block></value>
      </block>
      <block type="unityTransform_move">
        <value name="WHO"><block type="unity_me"></block></value>
        <value name="DELTA"><block type="unityVector_make">
          <value name="X"><block type="math_number"/></value>
          <value name="Y"><block type="math_number"/></value>
          <value name="Z"><block type="math_number"/></value>
        </block></value>
      </block>
      <block type="unityTransform_position">
        <value name="WHO"><block type="unity_me"></block></value>
      </block>
      <!--<block type="unityVector_angle"></block>-->
      
      <category name="Raycast (experimenteel)">
        <block type="unityRaycast_raycast">
          <value name="ORIGIN">              
            <block type="unityTransform_position">
              <value name="WHO"><block type="unity_me"></block></value>
            </block>
          </value>
          <value name="DIRECTION">
            <block type="unityVector_make">
              <value name="X"><block type="math_number"><field name="NUM">0</field></block></value>
              <value name="Y"><block type="math_number"><field name="NUM">-1</field></block></value>
              <value name="Z"></value>
            </block>
          </value>
          <value name="MAXDIST">
            <block type="math_number"><field name="NUM">1</field></block>
          </value>
        </block>
        <!--<block type="unityRaycast_circlecast">
          <value name="ORIGIN">              
            <block type="unityTransform_position">
              <value name="WHO"><block type="unity_me"></block></value>
            </block>
          </value>
          <value name="RADIUS">
            <block type="math_number"><field name="NUM">1</field></block>
          </value>
          <value name="DIRECTION">
            <block type="unityVector_make">
              <value name="X"><block type="math_number"><field name="NUM">0</field></block></value>
              <value name="Y"><block type="math_number"><field name="NUM">-1</field></value>
              <value name="Z"></value>
            </block>
          </value>
          <value name="MAXDIST">
            <block type="math_number"><field name="NUM">1</field></block>
          </value>
        </block>-->
        <block type="unityRaycast_gameobject"></block>
        <block type="unityRaycast_distance"></block>
        <block type="unityRaycast_point"></block>
        <block type="unityRaycast_normal"></block>
      </category>
      
    </category>
        
    <category name="Vallen, botsen, rollen" svg="true">
      <!-- TODO: add a lot more blocks here! -->
      <!-- + waarschuwing: rigidbody nodig! -->
      
      <note>Om deze blokken te laten werken heeft het object dat dit script heeft een RidgidBody2D en een soort Collidor2D nodig!</note>
      
      <block type="unityEvents_collision"></block>
      <block type="unityPhysics_collOther"></block>
      <block type="unityPhysics_collDirection"></block>
      <block type="unityPhysics_collSpeed"></block>
        
      <block type="unityPhysics_angularSpeed">
        <value name="WHO"><block type="unity_me"/></value>  
      </block>
        
      <block type="unityPhysics_velocity">
        <value name="WHO"><block type="unity_me"/></value>  
      </block>
        
      <block type="unityPhysics_push">
        <value name="WHO"><block type="unity_me"/></value>  
      </block>
        
      <block type="unityPhysics_torque">
        <value name="WHO"><block type="unity_me"/></value>  
      </block>
    </category>
    
    <category name="Clonen" svg="true">
      <block type="unityGeneral_destroy">
        <value name="WHO"><block type="unity_me"></block></value>
      </block>
      <block type="unityGeneral_create">
        <value name="WHO"><block type="unity_me"></block></value>
      </block>
      <block type="unityGeneral_createGet">
        <value name="WHO"><block type="unity_me"></block></value>
      </block>
    </category>

    <category name="Controle" svg="true">
        <block type="unityControls_if" />
        <block type="unityControls_ifElse" />
        
        <!-- Unity Messages => moved here -->
        <block type="unityGeneral_msgSend"></block>
        <block type="unityGeneral_msgReceive"></block>
        <!--<block type="unityGeneral_msgSendWithArg"></block>
        <block type="unityGeneral_msgReceiveWithArg"></block>-->
        
        
        <category_temp name="TOOLBOX_DECISION" svg="true">
          <!-- moved out of the category ^^^ -->
        </category_temp>
        <category_temp name="TOOLBOX_LOOP" svg="true">
            <!--<block type="robControls_loopForever" />-->
            <block type="controls_repeat_ext">
                <value name="TIMES">
                    <block type="math_number">
                        <field name="NUM">10</field>
                    </block>
                </value>
            </block>
            <!--<block type="controls_whileUntil" />-->
            <block type="unityControls_for">
                <value name="FROM">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
                <value name="TO">
                    <block type="math_number">
                        <field name="NUM">10</field>
                    </block>
                </value>
                <value name="BY">
                    <block type="math_number">
                        <field name="NUM">1</field>
                    </block>
                </value>
            </block>
            <block type="unityControls_forEach" />
            <block type="controls_flow_statements" />
        </category_temp>
    </category>
        
    <category name="Tijd" svg="true">
      <block type="unityTime_levelload"></block>
      <block type="unityTime_delta"></block>
    </category>
